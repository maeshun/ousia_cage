textarea:placeholder-shown{
    color: black
    vertical-align bottom
}
textarea::-webkit-input-placeholder{
    color: black
    font-weight bold
}
textarea:-moz-placeholder{
    color: black
    opacity: 1
    font-weight bold
}
textarea::-moz-placeholder{
    color: black
    opacity: 1
    font-weight bold
}
textarea:-ms-input-placeholder{
    color: black
    font-weight bold
}

:-moz-selection{background-color:#e1e1e1 ;color:#000;text-shadow:none}
::selection{background-color:#e1e1e1 ;color:#000;text-shadow:none}